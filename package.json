  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.7",
    "@popperjs/core": "^2.11.6",
    "acorn": "^8.8.1",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.3",
    "fs-extra": "^10.1.0",
    "jquery": "^3.6.3",
    "jquery-validation": "^1.19.5",
    "slick-slider": "^1.8.2"
  }
}
